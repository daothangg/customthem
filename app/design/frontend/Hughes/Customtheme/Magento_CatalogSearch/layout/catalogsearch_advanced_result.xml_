<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Magento\CatalogSearch\Block\Advanced\Result" name="catalogsearch_advanced_result" template="Magento_CatalogSearch::advanced/result.phtml">
			<referenceContainer name="after.start">
				<block class="Magento\CatalogSearch\Block\SearchResult\ListProduct" 
					   name="custom.category" 
					   before="-" 
					   template="Magento_CatalogSearch::BROWSE-BY_CATEGORY.phtml"
    />
</referenceContainer>
            </block>
        </referenceContainer>
       
    </body>
</page>
